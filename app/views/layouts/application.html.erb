<!DOCTYPE html>
<html>
  <head>
    <title>Airbnb</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>
  <body>      
<nav class="navbar navbar-default navbar-static-top">
    <div id="navbar">
    <%= image_tag "logo.png",height: 45, width: 45, class: "navbar-wagon-logo hidden-xs" %>
     
      <ul class="nav navbar-nav navbar-right">
        <li>
        <%=link_to "Host", hosts_url %>
        </li>
        <li>
           <%=link_to "Travelers", travelers_url %>
        </li>
         <li>
         <%=link_to "Recommendations" %>
        </li>
      </ul>
    </div>
  </div>
</nav>


    <%= yield %>
  </body>
</html>
